[Setup]
installType=global
acceptLicense=1
enableRemote=1
uninstallDir=@@uninstallDir@@

[Handlers]
installCF=1
installJSP=1
installWildCard=0

[Tomcat]
server=localhost
ajpPort=8009
configureServerXml=0

[Settings]
allowEmptyHeaders=0
forceSecureSession=0
enableHeaderDataSupport=1
ModCFMLSecret=@@secretkey@@

